{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": "off",
        "no-use-before-define": "warn",
        "import/no-cycle": "off",
        "import/extensions": "off",
        "import/no-mutable-exports": "off",      
        "no-param-reassign": "warn",
        "no-continue": "off",
    }
}
